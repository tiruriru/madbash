# Where's madbash located.
#-------------------------------------------------------------------------------
MADBASH=$HOME/.madbash

# Set theme.
#-------------------------------------------------------------------------------
THEME=default

# Load selected plugins.
#-------------------------------------------------------------------------------
PLUGINS='all'

# Select default editor.
#-------------------------------------------------------------------------------
export EDITOR='emacs -nw'

# Initialize madbash.
#-------------------------------------------------------------------------------
source $MADBASH/init.bash

# Custom path locations...
#-------------------------------------------------------------------------------
export PATH=$HOME/Scripts/:$PATH

